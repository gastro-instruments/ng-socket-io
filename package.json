		"@angular/compiler-cli": "^4.3.5",
		"@angular/core": "^4.3.5",
		"@angular/platform-server": "^4.3.5",
		"@ngrx/store": "^4.0.3",
		"@types/jasmine": "^2.5.53",
		"@types/jasminewd2": "^2.0.2",
		"@types/node": "^8.0.24",
