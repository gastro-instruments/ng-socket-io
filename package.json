		"karma-jasmine": "^1.1.0",
		"karma-jasmine-html-reporter": "^0.2.2",
		"karma-typescript": "^3.0.5",
		"normalizr": "^3.2.3",
		"rxjs": "^5.4.3",
		"should": "^11.2.1",
		"ts-node": "^3.3.0",
		"tslint": "^5.6.0",
		"typedoc": "^0.8.0",
		"typescript": "^2.4.2",
