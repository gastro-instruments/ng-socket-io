		"docs-github": "touch ./docs/.nojekyll",
		"test": "./node_modules/.bin/karma start karma-ci.conf.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/gastro-instruments/ng-socket-io"
	},
