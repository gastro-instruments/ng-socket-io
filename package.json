	"tags": ["angular", "socket.io", "typescript"],
	"readme": "README.md",
	"scripts": {
		"watch": "./node_modules/.bin/karma start",
		"prepublishOnly": "yarn aot",
		"tsc": "rm -rf ./dist && tsc",
		"aot": "rm -rf ./dist && ./node_modules/.bin/ngc",
